#
# To use this file, add the -i <path_to_this_file> option to your ansible command.  For example:
#
#  ansible -i /etc/ansible/static_hosts ChatOpsGlobal -m ping
#  ansible -i /etc/ansible/static_hosts VA1T3NCOPS-D03 -m ping
#  ansible -i /etc/ansible/static_hosts all -m ping
#  
#

